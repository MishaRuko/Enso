# HomeDesigner Environment Configuration
# Copy this file to .env and fill in actual values for local development

# OpenRouter API (Claude Sonnet for reasoning/intake, Gemini Vision for spatial analysis)
OPENROUTER_API_KEY=sk-or-v1-xxxxxxxxxxxxx

# LLM Models (via OpenRouter)
CLAUDE_MODEL=anthropic/claude-3.5-sonnet
GEMINI_MODEL=google/gemini-2.5-pro-exp-11-05

# Miro Board Generation (optional: stub URLs returned if missing)
MIRO_API_TOKEN=miro_token_xxxxxxxxxxxxx

# ElevenLabs Realtime Agent (optional: text-intake fallback used if missing)
ELEVENLABS_AGENT_ID=agent_xxxxxxxxxxxxx
ELEVENLABS_VOICE_ID=voice_xxxxxxxxxxxxx
ELEVENLABS_API_KEY=sk_xxxxxxxxxxxxx

# Backend Public URL (used in Miro board templates, ElevenLabs tool definitions)
BACKEND_PUBLIC_URL=http://localhost:8100

# Database (Supabase)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=eyJxxxxxxxxxxxx
SUPABASE_SERVICE_KEY=eyJxxxxxxxxxxxx

# Frontend & Hackathon Config
NEXT_PUBLIC_BACKEND_URL=http://localhost:8100
NEXT_PUBLIC_ELEVENLABS_AGENT_ID=agent_xxxxxxxxxxxxx
